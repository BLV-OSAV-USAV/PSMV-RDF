# ==============================================================================
# OWL METADATA
# ==============================================================================

ghs: a owl:Ontology ;
        dcterms:title "GHS phrase classification scheme"@en .

# ==============================================================================
# GHS CLASSIFICATION
# ==============================================================================

ghs:GHSStatement a owl:Class .

ghs:HazardPictogram a owl:Class ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/GHS_hazard_pictograms> ;
    rdfs:subClassOf ghs:GHSStatement .

ghs:HazardStatement a owl:Class ;
    rdfs:label "Hazard statements"@en ;
    schema:alternateName "R phrase"@en ;
    rdfs:comment "A hazard statement is a standardized description of the risks associated with hazardous chemical substances, indicating specific dangers such as toxicity or flammability. These were used in the European Union to denote risks on safety data sheets and labels."@en ;
    rdfs:subClassOf ghs:GHSStatement .

ghs:PrecautionaryStatement a owl:Class ;
    rdfs:label "Precautionary statements"@en ;
    schema:alternateName "S phrase"@en ;
    rdfs:comment "A precautionary statement is a standardized safety instruction for handling hazardous chemical substances. These phrases were used in the European Union to recommend safe handling, storage, and emergency measures on safety data sheets and labels."@en ;
    rdfs:subClassOf ghs:GHSStatement .

ghs:SignalWord a owl:Class ;
    rdfs:subClassOf ghs:GHSStatement .

# ==============================================================================
# GHS SIGNAL WORDS
# ==============================================================================

ghs:attention a ghs:SignalWord ;
    rdfs:label "Achtung"@de,
        "Warning"@en,
        "attention"@fr,
        "Attenzione"@it .

ghs:danger a ghs:SignalWord ;
    rdfs:label "Gefahr"@de,
        "Danger"@en,
        "danger"@fr,
        "Pericolo"@it .

